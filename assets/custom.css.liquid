@font-face {
    font-family: 'darinaregular';
    src: url('https://cdn.shopify.com/s/files/1/0823/0302/5461/files/darina_regular-webfont.woff2?v=1696253539') format('woff2'),
         url('https://cdn.shopify.com/s/files/1/0823/0302/5461/files/darina_regular-webfont.woff?v=1696253539') format('woff');
    font-weight: normal;
    font-style: normal;

}
/* Animation */
.animate{
  position: relative;
  opacity: 0;
}
.animated{
  opacity: 1;
  transition: .5s;
  -webkit-animation-delay: 20ms;
  animation-delay: 20ms;
}
.show_up{ top: 20px; }
.show_up.animated{
  top: 0;
}
.slide_left{ right: -20px; }
.slide_left.animated{ right: 0px; }
.slide_right{ left: -20px; }
.slide_right.animated{ left: 0px; }

.animated.ad-1{
  transition: 1s;
  -webkit-animation-delay: 30ms;
  transition-delay: 30ms;
}
.animated.ad-1-5{
  transition: 1.5s;
  -webkit-animation-delay: 60ms;
  animation-delay: 60ms;
}
.animated.ad-2{
  transition: 2s;
  -webkit-animation-delay: 90ms;
  animation-delay: 90ms;
}
.animated.ad-2-5 {
  transition: 2.5s;
  -webkit-animation-delay: 110ms;
  animation-delay: 110ms;
}
.animated.ad-3 {
  transition: 3s;
  -webkit-animation-delay: 120ms;
  animation-delay: 120ms;
}
/* Comman css */
.page-dots{
    --dot-size: 0.625rem;
}
/* Heading */
.sec_heading {
  text-transform: none;
  font-family: var(--head-font-family);
  font-size: 1.5rem;
  letter-spacing: 0;
}
.sub_heading {
  font-size: 2rem;
}
@media screen and (min-width: 1000px){
 .sec_heading {
   font-size: 2.5rem;
   letter-spacing: 4.2px;
 }
}
.container, .shopify-policy__container{
  max-width: 1200px;
  width: 100%;
  margin: 0 auto;
  padding-left: 20px;
  padding-right: 20px;
}
@media screen and (min-width: 1201px){
 .container, .shopify-policy__container{
    padding-left: 0;
    padding-right: 0;
 }
}
@media screen and (min-width: 1500px){
  .container, .shopify-policy__container{ max-width: 1440px; }
}

/* Header */
x-header.header {
  border-bottom: 0;
  box-shadow: none;
}
header {
  border-bottom: 1px solid rgb(212 212 206);
}
height-observer{
  background: rgb(var(--header-background));
}
.header__secondary-nav svg {
  max-height: 22px;
}
.mega-menu__linklist li ul li a {
  text-transform: uppercase;
  color: rgb(var(--text-color) / 1);
  border-bottom: 1px solid rgba(28,28,28,0.3);
  width: 100%;
  display: block;
  padding-bottom: 12px;
  padding-top: 8px;
  font-size: 0.875rem;
}
.mega-menu__promo {
  position: relative;
}
.mega-menu__promo a,.mega-menu__promo p{
  color: #fff;
}
.mega-menu__linklist li ul {
  width: 100%;
}
.mega-menu{
  justify-content: space-between;
}
.mega-menu__promo p.h6{
  font-size: var(--text-h1);
  font-family: var(--head-font-family);
}
.right-megamenu-image span{
  border-bottom: 1px solid #fff;
  text-transform: uppercase;
}
.mega-menu__linklist>li>a.h6 {
  font-size: var(--text-h3);
  text-transform: uppercase;
  font-family: var(--head-font-family);
  font-weight: 400;
  letter-spacing: 0.3px;
}
.mega-menu__linklist li ul li.stay-tune a {
  border-bottom: 0;
  color: #AFAFAF; 
}
.mega-menu__linklist li ul li:last-child a {
  border-bottom: 0;
}
.mega-menu__promo>*{
  max-width: 420px;
}
.mega-menu__linklist>* {
  flex: 1;
}
.mega-menu__linklist{
  flex: 0 0 70%;
  max-width: 70%;
  --mega-menu-linklist-gap: 2.5rem 2.60rem;
}
@media screen and (min-width: 1400px){
  .mega-menu {
    --mega-menu-gap: 0rem;
  }
}
@media screen and (min-width: 999px){
  .megamenu-image {
    display: flex;
    position: absolute;
    bottom: 0;
    justify-content: space-between;
    text-align: left;
    align-items: flex-end;
    width: 100%;
    padding: 20px 20px;
  }
}
@media screen and (max-width:999px){
  .right-megamenu-image span{
    border-bottom: 1px solid rgb(var(--text-color));
    font-size: 0.75rem;
  }
  .header-sidebar__footer svg {
    height: 15px;
  }
  .megamenu-image.v-stack {
    width: 100%;
    text-align: left;
  }
  .megamenu-image p.h6{
    font-size: var(--text-h3);
    font-family: var(--head-font-family);
  }
}
/* Footer */
.footer_logo {
  padding-bottom: 30px;
}
.footer__aside p {
  text-transform: capitalize;
}
.footer__aside {
  border-top: 1px solid rgb(var(--text-primary));
  padding-top: 20px;
  padding-bottom: 20px;
}
.footer-share{
  display: flex;
  justify-content: space-between;
}
.footer-instagram a.tap-area {
  display: flex;
  color: rgb(var(--text-primary));
  align-items: center;
}
.footer-instagram a.tap-area svg {
  margin-right: 10px;
  height: 30px;
  width: 30px;
}
.social-media--list .social-media__item{
  opacity: 1;
}
.payment-methods {
  align-items: center;
}
.Footer__PaymentList {
  display: flex;
  gap: 5px;
  margin-left: 0px;
}
.Footer__PaymentList li {
  display: block;
}
footer .text-subdued {
  color: rgb(var(--text-color) / 1);
}
footer .footer_menu_link a {
  color: rgb(var(--text-color) / 1);
  text-transform: uppercase;
}
footer {
  border-top: 1px solid rgb(var(--text-color));
}
.footer__aside {
  text-align: left;
  display: block;
}
.middle-footer {
  padding-top: 30px;
}
@media screen and (min-width: 1130px){
  .footer__block--text {
    max-width: 28rem;
    flex-basis: 28rem;
  }
}
@media screen and (min-width: 1400px){
  .footer__block-list {
    column-gap: 17rem;
    justify-content: flex-start;
  }
}
@media screen and (min-width: 1150px){
  footer .footer_menu_link {
    padding: 10px 0;
  }
}
@media screen and (max-width: 1400px){
  .footer__block-list {
    column-gap: 10rem;
    justify-content: flex-start;
  }
}
@media screen and (max-width: 1024px){
  .footer__block-list {
    column-gap: 6rem;
  }
}
@media screen and (max-width: 700px){
  .footer-share {
    flex-wrap: wrap;
  }
  .payment-methods {
    width: 100%;
    margin-top: 15px;
  }
  .footer_logo img {
    --image-max-width: 100px !important;
  }
  .middle-footer {
    padding-top: 0;
  }

}
/* Product page */
.product-gallery {
  gap: 1.5rem;
}
.shopify-section--main-product .product-title {
  text-transform: capitalize;
  font-weight: bold;
  font-size: 2rem;
}
compare-at-price.text-subdued {
  color: #DCDBD6;
}
.variant-picker__option-info legend {
  text-transform: uppercase;
  font-weight: bold;
}
label.quantity-label {
  width: 100%;
  display: block;
  font-weight: bold;
  text-transform: uppercase;
}
.product-info__block-item[data-block-type="buy-buttons"] .buy-buttons > button {
  border: 1px solid #000;
  color: #2B2B2B;
}
.product-info__block-item[data-block-type="accordion"] .accordion__toggle span{
  font-weight: bold;
}
.product-info__block-item[data-block-type="accordion"] .accordion__content ul {
  margin-left: 30px;
}
.product-info__block-group.accordion-group[data-group-type="accordion-group"] {
  --product-info-gap: 2rem;
}
product-recommendations .sec_heading{
  letter-spacing: 4.2px;
}
.product-info__block-item[data-block-type="feature-with-icon"],.product-usp {
  max-width: 33.33%;
  flex: 0 0 33.33%;
  padding: 0 5px;
}
.product-usps {
  margin: 0 -5px;
}
.product-usps-main {
  display: flex;
}
.feature-badge{
  width: 100%;
  justify-content: center;
  padding: 0.7rem 0.5rem;
}
.feature-badge-list{
  gap: 0;
}
.product-info {
  --product-info-gap: 0rem;
}
.complementary-products .price-list .text-subdued {
    color: #AFAFAF;
    font-size: 1.00rem;
}
.product-type{
  color: #AFAFAF;
  font-size: .90rem;
}
.complementary-products__header p{
  font-weight: bold;
}
.complementary-products {
  max-width: 100%;
}
.complementary-products .horizontal-product-card__figure {
    width: 9.25rem;
}
.complementary-products .view-product{
  text-transform: uppercase;
  font-weight: 500;
  letter-spacing: 2px;
  padding-top: 35px;
}
.complementary-products .product-title{
  font-size: 1.00rem;
}
sale-price.h6.text-subdued {
  color: #AFAFAF;
}
.top-arrow {
  text-align: center;
  margin: 0 auto;
}
.shopify-section--related-products {
  margin-top: 100px;
}
.size_chart{
  font-weight: 700;
  text-transform: uppercase;
  font-size: 0.9rem;
}
.size-chart-modal img{
  margin: 0 auto;
  display: block !important;
}
@media screen and (min-width: 999px) and (max-width:1500px){
  .product-info__block-item[data-block-type=feature-with-icon], .product-usp {
    max-width: 45%;
    flex: 0 0 45%;
    padding: 0 5px;
  }
  .product-usps {
    overflow-x: scroll;
    padding-bottom: 20px;
    padding-top: 20px;
  }
}
@media screen and (max-width: 700px){
  .shopify-section--main-product .product-title {
    font-size: var(--text-h4);
  }
  compare-at-price.h5 {
    font-size: var(--text-h6);
  }
  sale-price.h4 {
    font-size: var(--text-h5);
  }
  .product-info__block-list>:not(:last-child) {
    --product-info-gap: 0.5rem;
  }
}
@media screen and (max-width: 600px){
  .product-info__block-item[data-block-type=feature-with-icon], .product-usp {
    max-width: 49%;
    flex: 0 0 49%;
    padding: 0 5px;
  }
  .product-usps {
    overflow-x: scroll;
    padding-bottom: 20px;
    padding-top: 20px;
  }
}
/* Collection page */
.breadcrumb__list-item+.breadcrumb__list-item:before{
  content: none;
}
.breadcrumb__list-item a{
  display: flex;
  align-items: center;
}
.breadcrumb__list-item a svg {
  height: 11px;
  margin-right: 10px;
}
.breadcrumb {
  padding: 28px 0;
  text-transform: uppercase;
}
.breadcrumb__list-item {
  font-size: 0.9rem;
}
.collection-toolbar__button:hover, .collection-toolbar__button.is-active {
  opacity: 1;
  color: rgb(var(--text-color));
}
.collection-toolbar__button {
  opacity: 1;
  color: #AFAFAF;
  text-transform: capitalize;
}
p.collection-toolbar__products-count {
  color: #AFAFAF;
  opacity: 1;
  text-transform: capitalize;
}
.collection-toolbar__button span {
  color: #AFAFAF;
}
.heading_font span{
   display: block;
  padding-left: 133px;
  text-transform: capitalize;
  font-style: italic;
  font-size: 8rem;
}
.heading_font {
  text-transform: uppercase;
  font-size: 4rem;
  font-weight: 400;
  line-height: 78px;
}
.shopify-section--collection-image-with-text .prose {
  padding-right: 110px;
  padding-left: 0;
}
.shopify-section--collection-image-with-text .prose h3.h6 {
  max-width: 250px;
  font-size: var(--text-h5);
  padding: 40px 0;
}
.shopify-section--main-collection .product-list {
    position: relative;
    padding-bottom: 7rem;
}
.show_more {
  margin: 0 auto;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  padding: 20px 0;
  letter-spacing: 4.2px;
  max-width: 250px;
  align-items: center;
  display: flex;
}
.collection {
  padding-bottom: 100px;
}
.facets .accordion-list .accordion .accordion__toggle {
  font-weight: bold;
}
.accordion__content a {
  text-transform: uppercase;
  color: rgb(var(--text-color));
}
.image-content {
  padding-bottom: 40px;
}
.shopify-section--collection-image-with-text .prose {
  padding-right: 110px;
  padding-left: 135px;
}
.link.btn-hover-animation{
  font-size: var(--text-h4);
  letter-spacing: 3px;
}
.btn-hover-animation {
  background: none !important;
  display: flex !important;
  align-items: center;
  text-transform: uppercase;
}
.border-hover{
    width: 100%;
    max-width: 120px;
    height: 2px;
    display: block;
    background: linear-gradient(to right,white 50%, #767676 50%);
    background-size: 200% 100%;
    background-position: right bottom;
    transition: .5s ease-out;
    opacity: .5;
    margin-left: 15px;
}
.btn-hover-animation:hover span{
  background-position: left bottom;
  opacity: 1;
}
.product-card__info .product-title.h6 {
  font-weight: bold;
}
.dot-checkbox~label{
  opacity: 1;
}
@media screen and (min-width: 700px){
  .collection-toolbar__layout-switch-list {
    padding-inline: 0;
    padding-right: 8rem;
    gap: 1.5rem;
  }
}
@media screen and (max-width: 1450px){
  .shopify-section--collection-image-with-text .prose {
    padding-left: 80px;
  }
}
@media screen and (max-width: 1350px){
  .shopify-section--collection-image-with-text .prose {
    padding-left: 20px;
    padding-right: 25px;
  }
  .shopify-section--collection-image-with-text .prose h3.h6 {
    padding: 20px 0;
  }
  .heading_font {
    font-size: 3rem;
    line-height: 55px;
  }
  .heading_font span {
    padding-left: 104px;
    font-size: 5rem;
  }
  .shopify-section--collection-image-with-text .prose h3.h6 {
    padding: 20px 0;
  }
  .link.btn-hover-animation{
    font-size: var(--text-h5);
    letter-spacing: 2px;
  }
}
@media screen and (max-width: 1000px){
  .image-with-text{
    display:block;
  }
  .shopify-section--collection-image-with-text .prose{
    text-align: left;
    padding-right: 20px;
  }
}
@media screen and (max-width: 700px){
  .show_more {
    max-width: 190px;
    padding: 18px;
  }
}
/* Home page */
.shopify-section--home-banner .image-with-text--reverse>.prose {
  margin-inline-start: 0;
}
.shopify-section--home-banner {
  position: relative;
}
.shopify-section--home-banner .sec_heading span:nth-child(3) {text-align: right;display: block;}
.shopify-section--home-banner .sec_heading svg {
  text-align: center;
  margin: 0 auto;
}
.shopify-section--home-banner .sec_heading {
  max-width: 375px;
  margin: 0 auto;
  line-height: 40px;
  padding-bottom: 70px;
}
.shopify-section--home-banner .sec_heading span:nth-child(1) {
  margin-bottom: -25px;
  display: block;
}
.shopify-section--home-banner .sec_heading span {
  font-size: 4rem;
}
.shopify-section--home-banner  a.link.btn-hover-animation {
  max-width: 375px;
  margin: 0 auto;
  font-size: inherit;
}
.shopify-section--home-banner .image-with-text--reverse>.prose{
  padding-inline-start: 0;
}